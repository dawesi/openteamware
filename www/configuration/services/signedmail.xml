<service>
	<servicekey>signedmail</servicekey>
	<basedirectory>/signedmail/</basedirectory>
	<defaultaction>showwelcome</defaultaction>
	<frontend>www</frontend>
	<headerinclude>dsp_inc_top.cfm</headerinclude>
	<leftinclude>dsp_inc_left.cfm</leftinclude>
<actions>	
	<item>
		<name>showwelcome</name>
		<description>show welcome screen</description>
		<template>dsp_welcome.cfm</template>
		<checkframeset>0</checkframeset>
		<setservice>0</setservice>
		<directory></directory>
		<productkeysincluded></productkeysincluded>
		<attributes></attributes>
	</item>	
	<item>
		<name>information</name>
		<description></description>
		<template>dsp_information.cfm</template>
		<checkframeset>0</checkframeset>
		<setservice>0</setservice>
		<directory></directory>
		<productkeysincluded></productkeysincluded>
		<attributes></attributes>
	</item>		
	<item>
		<name>certsearch</name>
		<description></description>
		<template>dsp_cert_search.cfm</template>
		<checkframeset>1</checkframeset>
		<setservice>0</setservice>
		<directory></directory>
		<productkeysincluded></productkeysincluded>
		<attributes></attributes>
	</item>	
	<item>
		<name>verifymail</name>
		<description></description>
		<template>dsp_verify_mail.cfm</template>
		<checkframeset>1</checkframeset>
		<setservice>0</setservice>
		<directory></directory>
		<productkeysincluded></productkeysincluded>
		<attributes></attributes>
	</item>
</actions>
