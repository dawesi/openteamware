		<directory></directory>
		<productkeysincluded></productkeysincluded>
		<attributes></attributes>
	</item>
	<item>
		<name>newitem</name>
		<description></description>
		<template>dsp_newitem.cfm</template>
		<checkframeset>1</checkframeset>
		<setservice>0</setservice>
		<directory></directory>
		<productkeysincluded></productkeysincluded>
		<attributes></attributes>
	</item>
</actions>
</service>